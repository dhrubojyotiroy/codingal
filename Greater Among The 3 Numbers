a = int(input("Enter the first number: "))
b = int(input("Enter the second number: "))
c = int(input("Enter the third number: "))
if a > b and a > c:
    print("A has the highest value which is",a,".")
elif b > c:
    print("B has the highest value which is",b,".")
else:
    print("C has the highest value which is",c,".")
